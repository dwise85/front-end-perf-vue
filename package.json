{
  "name": "vue-performance-app-test",
  "description": "Simple app built to test performance",
  "version": "1.0.0",
  "author": "Don Wise",
  "private": true,
  "sideEffects": true,
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "scripts": {
    "build": "cross-env-shell NODE_ENV=production npm run clean && npm run lint && npm run test && npm run compile",
    "clean": "rimraf dist && rimraf coverage",
    "analyze": "webpack-bundle-analyzer dist/stats.json --report dist/report.html -m static",
    "compile": "webpack --config build/webpack.config.prod.js",
    "compile:analyze": "npm run clean && npm run compile && npm run analyze",
    "coverage": "npm run coverage:run && npm run coverage:remap && npm run coverage:open",
    "coverage:open": "start coverage/html-ts/index.html",
    "coverage:remap": "remap-istanbul -i coverage/json/coverage-final.json -o coverage/html-ts -t html",
    "coverage:run": "cross-env NODE_ENV=development karma start build/karma.coverage.js",
    "dev": "webpack-dev-server --config build/webpack.config.dev.js --hot",
    "prod": "webpack-dev-server --config build/webpack.config.prod.js",
    "lint": "eslint --ext .js,.vue,.ts src build",
    "serve": "http-server dist/ -g -o",
    "test": "cross-env NODE_ENV=development karma start build/karma.unit.js",
    "test:debug": "cross-env NODE_ENV=development karma start build/karma.debug.js",
    "test:watch": "cross-env NODE_ENV=development karma start build/karma.unit.js --singleRun=false --auto-watch"
  },
  "dependencies": {
    "bootstrap-vue": "^2.0.0-rc.28",
    "vue": "^2.6.10",
    "vue-property-decorator": "^8.2.1",
    "vue-router": "^3.1.1",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/lodash": "^4.14.136",
    "@types/lodash.merge": "^4.6.6",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.1",
    "@types/sinon": "^7.0.13",
    "@types/vue": "^2.0.0",
    "@types/webpack-env": "^1.13.5",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "autoprefixer": "^9.6.1",
    "chai": "^4.1.2",
    "compression-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.1.3",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsdoc": "^15.8.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "favicons-webpack-plugin": "1.0.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "json-loader": "^0.5.7",
    "karma": "^6.3.16",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon": "^1.0.5",
    "karma-source-map-support": "^1.2.0",
